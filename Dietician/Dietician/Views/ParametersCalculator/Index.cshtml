@model Parameters

@{
    ViewData["Title"] = "Obliczanie parametrów";
}

@using (Html.BeginForm())
{
    <div class="container">
        @Html.Partial("parameters", Model, new ViewDataDictionary(ViewContext.ViewData) { TemplateInfo = { HtmlFieldPrefix = "Model" } })
    </div>

    @if (Model.ShowResults)
    {
        <div class="container">
            @Html.Partial("results", Model, new ViewDataDictionary(ViewContext.ViewData) { TemplateInfo = { HtmlFieldPrefix = "Model" } })
        </div>
    }
}