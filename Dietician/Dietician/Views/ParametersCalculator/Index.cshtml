@model Parameters

@{
    ViewData["Title"] = "Obliczanie parametrów";
}

@using (Html.BeginForm())
{
    <div class="container">
        @Html.Partial("parameters", Model)
    </div>

    @if (Model.ShowResults)
    {
      <div class="container">
        @Html.Partial("results", Model)
    </div>
     }
 }